            @Html.DisplayFor(modelItem => item.dtRequested)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dtSchedule)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dtPerformed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HisProfile.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HisRequest.Title)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
