        <th>
            @Html.DisplayNameFor(model => model.dtSchedule)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dtPerformed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarks)
